{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "plugins": [
    "react",
    "flowtype"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": "off",
    "react/prop-types": [
      "error",
      {
        "ignore": [
          "navigation"
        ]
      }
    ]
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}
